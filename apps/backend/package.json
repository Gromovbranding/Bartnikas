{
   "name": "@bartnikas/backend",
   "version": "0.0.1",
   "private": true,
   "engines": {
      "node": ">=20.9.0"
   },
   "scripts": {
      "build": "cross-env NODE_ENV=production nest build",
      "start:dev": "cross-env NODE_ENV=development nest start --watch",
      "start:prod": "node --env-file .env.local ./dist/main.js",
      "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
      "format": "prettier --write \"src/**/*.ts\"",
      "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && npm run format",
      "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
      "typeorm": "ts-node ./node_modules/typeorm/cli",
      "migration:run": "npm run typeorm migration:run -- -d ./src/typeOrm.config.ts",
      "migration:generate": "npm run typeorm -- -d ./src/typeOrm.config.ts migration:generate ./src/migrations/___new-migration___",
      "migration:create": "npm run typeorm -- migration:create ./src/migrations/___new-migration___",
      "migration:revert": "npm run typeorm -- -d ./src/typeOrm.config.ts migration:revert"
   },
   "dependencies": {
      "@nestjs-modules/mailer": "^1.8.1",
      "@nestjs/common": "^9.0.0",
      "@nestjs/config": "^2.3.1",
      "@nestjs/core": "^9.0.0",
      "@nestjs/jwt": "^10.0.3",
      "@nestjs/mapped-types": "*",
      "@nestjs/platform-express": "^9.0.0",
      "@nestjs/swagger": "^6.3.0",
      "@nestjs/typeorm": "^9.0.1",
      "bcrypt": "^5.1.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "cross-env": "^7.0.3",
      "dotenv": "^16.0.3",
      "nodemailer": "^6.9.3",
      "pg": "^8.10.0",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.2.0",
      "stripe": "^12.8.0",
      "typeorm": "0.3.15",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@nestjs/cli": "^9.0.0",
      "@nestjs/schematics": "^9.0.0",
      "@nestjs/testing": "^9.0.0",
      "@types/bcrypt": "^5.0.0",
      "@types/express": "^4.17.13",
      "@types/jest": "29.2.4",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/nodemailer": "^6.4.8",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "29.3.1",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "29.0.3",
      "ts-loader": "^9.2.3",
      "ts-node": "10.7.0",
      "tsconfig-paths": "^4.1.1",
      "typescript": "4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
